{
  "schemaVersion": 1,
  "id": "twitchlink",
  "version": "${version}",
  "name": "TwitchLink",
  "description": "Link Twitch and Minecraft together!",
  "authors": [
    "Cosmic"
  ],
  "contact": {
    "website": "https://cosrnic.dev",
    "repo": "https://github.com/cosrnic/TwitchLink"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/twitchlink/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.cosrnic.twitchlink.TwitchLink"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.10.8+kotlin.1.9.0"
  }
}
